	<section id="register-panel">
      <div class="container">
      <div class="border-top"></div>
        <!-- Return Customer  -->
        <div class="col-xs-12 col-sm-12 col-md-7 col-lg-7">
            <div class="return-costomer-register">
            <h3>Register Account</h3>
            <h5>If you already have an account with us, please login at the <a href="/login">login page.</a></h5>
            <!-- facebook login -->
            <i class="fa fa-facebook"></i> 
            <button type="button" class="btn btn-primary btn-block active btn-facebook">Login with Facebbok</button>
            <!-- /facebook login -->
            <h2>OR</h2>
            <!-- 	form login -->
            <div class="form-register">
            <form method="post" action="/signup">
                <div class="form-header">
                    <div class="title">Your Personal Details</div>
                    <div class="border-buttom"></div>
                </div>                                  
                <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <label for="username"><span class="require">*</span>Username</label>
                    <input name="name" id="username" type="text" class="form-control" placeholder="Enter username"/>
                </div>                                    
                <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <label for="email"><span class="require">*</span>Email</label>
                    <input name="email"  id="email" class="form-control" type="text" placeholder="Enter email"/>
                </div>                  
                <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <label for="passowrd">Password</label>
                    <input name="password" id="password"  class="form-control" type="password" placeholder="***************"/>
                </div>                                     
                <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <label for="confirm-passowrd">Confirm Password</label>
                        <input name="password" id="confirm-password"  class="form-control" type="password" placeholder="***************"/>
                </div>                 
                <div class="form-group col-xs-12 col-sm-12">
                        <label for="phone"><span class="require">*</span>YOUR PHONE NUMBER</label>
                        <input type="text" class="form-control" id="phone" placeholder="010888***" ng-model="phoneNumber"> 
                </div>                       
                <div class="form-group col-xs-12 col-sm-12 col-md-8 col-lg-8">
                        Yes <input type="checkbox" checked="checked"> No
                </div>
                <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="checkbox">
                      I have read and agree to the <a href="#">Privacy Policy</a><input type="checkbox">
                    </div>
                </div>                                   
                <input type="submit"  class="btn btn-success btn-block btn-new-register" value="Continue"/>               
            </form>
        </div>
        <!-- form login -->
    </div>
</div>
<!-- /Return Customer -->

